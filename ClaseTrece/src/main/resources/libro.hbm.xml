<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping>
	<class name="org.indra.models.Libro" table="Book">
		<id name="id">
			<generator class="increment"></generator>
		</id>
		<property name="titulo" column="title"></property>
		<property name="escritor" column="author"></property>
	</class>
</hibernate-mapping>