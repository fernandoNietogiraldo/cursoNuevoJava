<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping>
	<class name="org.indra.models.Persona" table="Persona">
		<id name="id">
			<generator class="increment"></generator>
		</id>
		<property name="nombre" column="nombre"></property>
		<property name="apellido" column="apellido"></property>
	</class>
</hibernate-mapping>